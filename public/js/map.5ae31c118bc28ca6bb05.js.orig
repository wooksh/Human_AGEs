      ArcheoSession.removeDataset(datasetId);
      ArcheoEvents.broadcast("dataset-update", '#layer-template');
      /* Remove layer from cache */

      ArcheoCache.removeDataset(datasetId);
      /* Remove layer from interface */

      $dataset.remove();
    }
  });
}

