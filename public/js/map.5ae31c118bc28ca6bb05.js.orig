function addGroupToLegend(legend, groupName) {
  if (!(groupName in legend)) {
    legend[groupName] = {
      name: groupName,
      filtered: false,
      color: ArcheoUtilities.randomRGBColorGenerator().toRgbString(),
      group: null
    };
  }
